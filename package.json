{
  "name": "delulu",
  "productName": "DELULU",
  "version": "1.0.0",
  "main": "main.js",
  "type": "module",
  "author": {
    "name": "Botsailer",
    "email": "anupm019@gmail.com"
  },
  "homepage": "https://github.com/botsailer/delulu",
  "license": "MIT",
  "description": "Entertainment App - Anime, Movies & More",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"cd renderer && npm run dev\" \"wait-on http://localhost:5173 && nodemon --exec electron .\"",
    "build:renderer": "cd renderer && npm run build",
    "obfuscate": "node scripts/obfuscate.js",
    "build:linux": "npm run build:renderer && npx electron-builder --linux",
    "build:win": "npm run build:renderer && npx electron-builder --win",
    "build:mac": "npm run build:renderer && npx electron-builder --mac",
    "build:all": "npm run build:renderer && npx electron-builder --linux --win",
    "build:linux-dev": "npm run build:renderer && npx electron-builder --linux",
    "build:win-dev": "npm run build:renderer && npx electron-builder --win",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "appId": "com.delulu.app",
    "productName": "DELULU",
    "directories": {
      "output": "dist-electron"
    },
    "compression": "maximum",
    "removePackageScripts": true,
    "npmRebuild": false,
    "nodeGypRebuild": false,
    "buildDependenciesFromSource": false,
    "files": [
      "main.js",
      "preload.js",
      "api-handler.js",
      "anime-handler.js",
      "manga-handler.js",
      "movie-handler.js",
      "package.json",
      "assets/icon.*",
      "renderer/dist/**/*",
      "!renderer/src",
      "!renderer/public",
      "!renderer/node_modules",
      "!renderer/index.html",
      "!renderer/vite.config.js",
      "!renderer/package.json",
      "!renderer/postcss.config.js",
      "!renderer/tailwind.config.js",
      "!renderer/eslint.config.js",
      "!renderer/README.md",
      "!**/*.map",
      "!**/*.ts",
      "!**/*.md",
      "!**/*.txt",
      "!**/LICENSE*",
      "!**/license*",
      "!**/CHANGELOG*",
      "!**/changelog*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme,*.txt}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples,samples}",
      "!**/node_modules/*/{.bin,.cache,.nyc_output,coverage,benchmark,fixtures}",
      "!**/node_modules/**/*.d.ts",
      "!**/node_modules/**/*.map",
      "!**/node_modules/**/*.md",
      "!**/node_modules/**/docs/**",
      "!**/node_modules/**/doc/**",
      "!**/node_modules/**/.github/**",
      "!**/node_modules/**/man/**",
      "!**/node_modules/**/bin/**",
      "!**/node_modules/**/{jest,mocha,chai,sinon}/**",
      "!**/node_modules/**/src/**",
      "!**/node_modules/**/esm/**",
      "!**/node_modules/**/*.ts",
      "!**/node_modules/**/*.mts",
      "!**/node_modules/**/tsconfig.json",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "asar": true,
    "asarUnpack": [
      "assets/icon.*"
    ],
    "electronLanguages": ["en-US"],
    "linux": {
      "target": [
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "assets/icon.png",
      "category": "AudioVideo",
      "artifactName": "${productName}-${version}-${arch}.${ext}"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "assets/icon.ico",
      "artifactName": "${productName}-${version}-Setup.${ext}"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "assets/icon.icns",
      "artifactName": "${productName}-${version}.${ext}"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "deleteAppDataOnUninstall": true,
      "installerIcon": "assets/icon.ico",
      "uninstallerIcon": "assets/icon.ico",
      "artifactName": "${productName}-${version}-Setup.${ext}"
    }
  },
  "dependencies": {
    "@consumet/extensions": "^1.8.7"
  },
  "devDependencies": {
    "@electron/asar": "^3.4.1",
    "@eslint/js": "^9.39.1",
    "@lottiefiles/dotlottie-react": "^0.17.12",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "javascript-obfuscator": "^5.1.0",
    "motion": "^12.23.26",
    "nodemon": "^3.1.11",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.2.0",
    "tailwindcss": "^3.4.19",
    "vite": "^7.2.4",
    "wait-on": "^9.0.3",
    "zustand": "^5.0.9"
  }
}
